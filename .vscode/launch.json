{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "rust: cargo clippy",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "Launch debug nRF52840",
      "flashingConfig": {
        "flashingEnabled": true,
      },
      "chip": "nRF52840_xxAA",
      "coreConfigs": [
        {
          "programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/mt-rt"
        }
      ]
    }
  ]
}