{
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.check.allTargets": false,
    "rust-analyzer.check.extraArgs": [
        "--bins"
    ],
    "VsCodeTaskButtons.tasks": [
        {
            "label": "$(play) Run",
            "task": "cargo run"
        },
        {
            "label": "$(check) Test",
            "task": "cargo test"
        }
    ]
}